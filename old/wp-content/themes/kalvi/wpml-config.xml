<wpml-config>

    <custom-fields>
		<custom-field action="copy">_dt_post_settings</custom-field>
		<custom-field action="copy">_portfolio_settings</custom-field>
		<custom-field action="copy">_tpl_default_settings</custom-field>
		<custom-field action="copy">_custom_settings</custom-field>
        <custom-field action="copy">_dt_custom_settings</custom-field>
        <custom-field action="translate">course-curriculum</custom-field>
        <custom-field action="translate">dtlms-class-maintabtitle</custom-field>
        <custom-field action="translate">dtlms-class-content-title</custom-field>
        <custom-field action="translate">dtlms-class-shortcode</custom-field>
        <custom-field action="translate">dtlms-class-address</custom-field>
        <custom-field action="translate">dtlms-class-accessories-tabtitle</custom-field>
        <custom-field action="translate">dtlms-class-accessories-label</custom-field>
        <custom-field action="translate">dtlms-class-accessories-value</custom-field>
        <custom-field action="translate">dtlms-class-accessories-description</custom-field>
        <custom-field action="translate">dtlms-class-tabs-title</custom-field>
        <custom-field action="translate">dtlms-class-tabs-content</custom-field>
        <custom-field action="translate">sidebar-content</custom-field>
        <custom-field action="translate">media-attachment-titles</custom-field>
        <custom-field action="translate">sidebar-content</custom-field>
        <custom-field action="translate">lesson-curriculum</custom-field>
        <custom-field action="translate">quiz-subtitle</custom-field>
        <custom-field action="translate">quiz-postmsg</custom-field>
        <custom-field action="translate">multichoice-answers</custom-field>
        <custom-field action="translate">multicorrect-answers</custom-field>
        <custom-field action="translate">text-before-gap</custom-field>
        <custom-field action="translate">gap</custom-field>
        <custom-field action="translate">text-after-gap</custom-field>
        <custom-field action="translate">singleline-answer</custom-field>
        <custom-field action="translate">multiline-answer</custom-field>
        <custom-field action="translate">answer-hint</custom-field>
        <custom-field action="translate">answer-explanation</custom-field>
        <custom-field action="translate">assignment-subtitle</custom-field>
        <custom-field action="translate">review-or-feedback</custom-field>
    </custom-fields>

    <custom-types>
        <custom-type translate="1">dt_portfolios</custom-type>
        <custom-type translate="1">dtlms_classes</custom-type>
        <custom-type translate="1">dtlms_courses</custom-type>
        <custom-type translate="1">dtlms_lessons</custom-type>
        <custom-type translate="1">dtlms_quizzes</custom-type>
        <custom-type translate="1">dtlms_questions</custom-type>
        <custom-type translate="1">dtlms_assignments</custom-type>
        <custom-type translate="1">dtlms_certificates</custom-type>
        <custom-type translate="1">dtlms_gradings</custom-type>
        <custom-type translate="1">dtlms_packages</custom-type>
        <custom-type translate="1">dtlms_payments</custom-type>
        <custom-type translate="1">dt_headers</custom-type>
        <custom-type translate="1">dt_footers</custom-type>
    </custom-types>

    <taxonomies>
        <taxonomy translate="1">portfolio_entries</taxonomy>
        <taxonomy translate="1">portfolio_tags</taxonomy>
        <taxonomy translate="1">course_category</taxonomy>
        <taxonomy translate="1">question_category</taxonomy>
		<taxonomy translate="0">dt_dummy</taxonomy>
    </taxonomies>

    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>    
        <key name="icl_language_switcher_sidebar">0</key>  
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>    
    </language-switcher-settings>
    <admin-texts>
        <key name="_cs_options">
			<key name="post-archives-readmore"/>
			<key name="top-hook"/>
			<key name="content-before-hook"/>
			<key name="content-after-hook"/>
			<key name="bottom-hook"/>
            <key name="cookie-consent-msg"/>
            <key name="privacy-commentform-msg"/>
            <key name="privacy-subscribeform-msg"/>
            <key name="privacy-loginform-msg"/>
            <key name="cookie-bar-buttons"/>
            <key name="custom-model-heading"/>
            <key name="custom-model-tabs"/>
			<key name="single-portfolio-slug"/>
			<key name="portfolio-category-slug"/>
        </key>
    </admin-texts>
</wpml-config>